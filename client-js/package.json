{
  "private": true,
  "name": "ledger-libs-icx",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "BABEL_ENV=production && lerna run build",
    "clean": "rm -rf node_modules packages/*/node_modules/ packages/*/flow-typed/ packages/*/lib flow-typed",
    "documentation": "documentation build packages/hw-*/src/** --project-name '@ledgerhq/*' --project-version `node -e \"console.log(require('./lerna.json').version)\"` --project-homepage https://repo.theloop.co.kr/enterprise/ledger-app-icx --config documentation.yml -g -f html -o packages/documentation-website/public/docs", 
    "lint": "eslint packages/**/src",
    "rebuild": "lerna run clean && lerna run build", 
    "test": "cd packages/test-icx && yarn run test",
    "watch": "lerna run --parallel watch"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browserify": "^16.1.1",
    "create-hash": "^1.1.3",
    "documentation": "^6.1.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-react": "^7.6.1",
    "flow-bin": "^0.68.0",
    "flow-copy-source": "^1.2.2",
    "flow-mono-cli": "^1.3.0",
    "flow-typed": "^2.4.0",
    "lerna": "2.9.0",
    "prettier": "^1.10.2",
    "uglify-js": "^3.3.15"
  }
}
